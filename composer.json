{
	"name": "wikibase-solutions/semantic-structured-discussions",
	"type": "mediawiki-extension",
	"description": "Adds semantic annotations to Structured Discussions topics",
	"keywords": [
		"mediawiki",
		"semantic",
		"semantic-mediawiki"
	],
	"homepage": "http://www.mediawiki.org/wiki/Extension:Semantic_Structured_Discussions",
	"license": "GPL-2.0-or-later",
	"authors": [
		{
			"name": "Marijn van Wezel",
			"homepage": "https://marijn.it/",
			"role": "Original author"
		}
	],
	"support": {
		"email": "info@wikibase.nl"
	},
	"require": {
		"php": ">=7.4.0"
	},
	"require-dev": {
		"mediawiki/mediawiki-codesniffer": "34.0.0",
		"mediawiki/mediawiki-phan-config": "0.10.6",
		"mediawiki/minus-x": "1.1.1",
		"php-parallel-lint/php-console-highlighter": "0.5.0",
		"php-parallel-lint/php-parallel-lint": "1.3.0"
	},
	"scripts": {
		"lint": "parallel-lint . --exclude vendor --exclude node_modules --exclude .git",
		"phpcs": "phpcs -p -s",
		"minusx": "minus-x check .",
		"phpunit": "php ../../tests/phpunit/phpunit.php -c phpunit.xml.dist",
		"test": "composer phpunit tests/"
	},
	"config": {
		"process-timeout": 600
	}
}
